
module Game


start(){
    write("thread started")
    game g("my Window Title!",800,800)

    Player p("./assets/mario.png",{},DefaultMoveKeys,1,1,true)

    g.addPlayer(p)
    p.OnMove([&](Position old, Position n){
        write("new position:",n.x,n.y)
    })
    write(p.getScale().x,p.getScale().y)
    g.Start()
    write("thread done")
}